<p id="notice"><%= notice %></p>

<h1>Identificacions</h1>

<table>
  <thead>
    <tr>
      <th>Edifici</th>
      <th>Tipus via</th>
      <th>Nom via</th>
      <th>Numero via</th>
      <th>Bloc</th>
      <th>Codi postal</th>
      <th>Poblacio</th>
      <th>Provincia</th>
      <th>Any construccio</th>
      <th>Superficie util</th>
      <th>Nom propietari</th>
      <th>Cognoms propietari</th>
      <th>Tipus document identitat propietari</th>
      <th>Numero document identitat propietari</th>
      <th>Email propietari</th>
      <th>Telefon propietari</th>
      <th>Adreca propietari</th>
      <th>Codi postal propietari</th>
      <th>Municipi propietari</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identificacions.each do |identificacio| %>
      <tr>
        <td><%= identificacio.edifici_id %></td>
        <td><%= identificacio.tipus_via %></td>
        <td><%= identificacio.nom_via %></td>
        <td><%= identificacio.numero_via %></td>
        <td><%= identificacio.bloc %></td>
        <td><%= identificacio.codi_postal %></td>
        <td><%= identificacio.poblacio %></td>
        <td><%= identificacio.provincia %></td>
        <td><%= identificacio.any_construccio %></td>
        <td><%= identificacio.superficie_util %></td>
        <td><%= identificacio.nom_propietari %></td>
        <td><%= identificacio.cognoms_propietari %></td>
        <td><%= identificacio.tipus_document_identitat_propietari %></td>
        <td><%= identificacio.numero_document_identitat_propietari %></td>
        <td><%= identificacio.email_propietari %></td>
        <td><%= identificacio.telefon_propietari %></td>
        <td><%= identificacio.adreca_propietari %></td>
        <td><%= identificacio.codi_postal_propietari %></td>
        <td><%= identificacio.municipi_propietari %></td>
        <td><%= link_to 'Show', identificacio %></td>
        <td><%= link_to 'Edit', edit_identificacio_path(identificacio) %></td>
        <td><%= link_to 'Destroy', identificacio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

